	<!-- ===== Ultratech Boots ===== -->



	<!-- ===== Eltex ===== -->

	<ThingDef ParentName="PsychicApparelBase" MayRequire="Ludeon.Rimworld.Royalty">
		<defName>CAT_Footwear_EltexBoots</defName>
		<label>eltex boots</label>
		<description>A pair of boots embedded with special psychic focusing materials. While not very protective, it enhances the wearer's psychic capabilities and helps to dissipate neural heat.</description>
		<graphicData>
			<texPath>Things/Footwear/CAT_EltexBoots</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<tradeability>Sellable</tradeability>
		<statBases>
			<MarketValue>300</MarketValue>
			<MaxHitPoints>80</MaxHitPoints>
			<Mass>0.15</Mass>
			<Flammability>0.4</Flammability>
			<ArmorRating_Sharp>0.05</ArmorRating_Sharp>
			<ArmorRating_Blunt>0</ArmorRating_Blunt>
			<ArmorRating_Heat>0.03</ArmorRating_Heat>
			<Insulation_Cold>3</Insulation_Cold>
			<Insulation_Heat>1</Insulation_Heat>
			<EquipDelay>1.0</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<PsychicSensitivityOffset>0.1</PsychicSensitivityOffset>
			<PsychicEntropyRecoveryRate>0.033</PsychicEntropyRecoveryRate>
		</equippedStatOffsets>
		<thingCategories Inherit="False">
			<li>CAT_BootsAndGloves</li>
		</thingCategories>
		<apparel>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
			<developmentalStageFilter>Child, Adult</developmentalStageFilter>
			<bodyPartGroups>
				<li>Feet</li>
			</bodyPartGroups>
			<layers>
				<li>Middle</li>
			</layers>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<only>(57,87,93)</only>
				</li>
				<li>
					<only>(51,27,49)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>



