	<!-- ===== Recon ===== -->

	<ThingDef ParentName="ApparelMakeableBase">
		<defName>CAT_Footwear_ReconBoots</defName>
		<label>recon boots</label>
		<description>A pair of light power armor boots, fitted with strategically placed plasteel plates and servomotors. Their relatively light weight allows the servomotors to work without hinderance.</description>
		<graphicData>
			<texPath>Things/Footwear/CAT_ReconBoots</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<techLevel>Spacer</techLevel>
		<tradeability>Sellable</tradeability>
		<generateCommonality>0.25</generateCommonality>
		<costList>
			<Plasteel>5</Plasteel>
			<ComponentSpacer>1</ComponentSpacer>
		</costList>
		<statBases>
			<MaxHitPoints>140</MaxHitPoints>
			<WorkToMake>12000</WorkToMake>
			<Mass>2.5</Mass>
			<Flammability>0.4</Flammability>
			<ArmorRating_Sharp>0.74</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.32</ArmorRating_Blunt>
			<ArmorRating_Heat>0.37</ArmorRating_Heat>
			<Insulation_Cold>2</Insulation_Cold>
			<Insulation_Heat>4</Insulation_Heat>
			<EquipDelay>1.5</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>0.1</MoveSpeed>
		</equippedStatOffsets>
		<thingCategories Inherit="False">
			<li>CAT_BootsAndGloves</li>
		</thingCategories>
		<apparel>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
			<developmentalStageFilter>Child, Adult</developmentalStageFilter>
			<bodyPartGroups>
				<li>Feet</li>
			</bodyPartGroups>
			<layers>
				<li>Middle</li>
			</layers>
			<tags>
				<li>SpacerMilitary</li>
				<li>RoyalTier2</li>
				<li>RoyalTier3</li>
				<li>RoyalTier4</li>
				<li>RoyalTier5</li>
				<li>RoyalTier6</li>
				<li>RoyalTier7</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
			<useDeflectMetalEffect>true</useDeflectMetalEffect>
			<soundWear>Wear_PowerArmor</soundWear>
			<soundRemove>Remove_PowerArmor</soundRemove>
		</apparel>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
			<researchPrerequisite>ReconArmor</researchPrerequisite>
			<skillRequirements>
				<Crafting>6</Crafting>
			</skillRequirements>
			<recipeUsers Inherit="False">
				<li>FabricationBench</li>
			</recipeUsers>
